<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<!-- new -> others -> Spring Bean Configuration File -->

<bean id="col" class="com.collection.MyCollectionImpl">
	<property name="array">
		<array>
			<value>100</value>
			<value>200</value>
			<value>300</value>
		</array>
	</property>
	
	<property name="list">
		<list>
			<value>red</value>
			<value>blue</value>
			<value>yellow</value>
			<value>white</value>
			<value>black</value>
		</list>
	</property>
	
	<property name="set">
		<set>
			<value>james</value>			
			<value>kim</value>			
			<value>bob</value>			
		</set>
	</property>
	
	<property name="map">
		<map>
			<entry>
				<key><value>num</value></key>
				<value>123</value>
			</entry>
			<entry>
				<key><value>name</value></key>
				<value>lee june</value>
			</entry>
			<entry>
				<key><value>address</value></key>
				<value>london</value>
			</entry>
		</map>
	</property>
</bean>

</beans>
